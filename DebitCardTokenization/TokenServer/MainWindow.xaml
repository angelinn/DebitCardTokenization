<Window x:Class="TokenServer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Closed="Window_Closed">
    <Grid>
        <TextBox x:Name="txtID" HorizontalAlignment="Left" Height="32" Margin="54,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="276" Text="Bank ID"/>
        <TextBox x:Name="txtToken" HorizontalAlignment="Left" Height="32" Margin="54,163,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="276" Text="Token"/>
        <Button x:Name="btnCheck" Content="Generate!" HorizontalAlignment="Left" Margin="390,96,0,0" VerticalAlignment="Top" Width="75" Click="btnCheck_Click"/>

    </Grid>
</Window>
